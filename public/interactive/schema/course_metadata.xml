	<metametadata>
		<metadatascheme>ADL SCORM(TM) 1.2</metadatascheme>
	</metametadata>

	<technical>
		<format>text/html</format>
		<location type="URI">https://ile-support.nko.navy.mil</location>

		<hstm:HSTMConfiguration xmlns="http://www.healthstream.com/HSTMConfiguration.xsd">

			<controls>
				<showFinishButton>yes</showFinishButton>
				<showHelp>no</showHelp>
				<showProgressBar>no</showProgressBar>
				<showCourseStructure>no</showCourseStructure>
				<courseStructureStartsOpen>no</courseStructureStartsOpen>
				<showNavBar>yes</showNavBar>
				<showTitleBar>yes</showTitleBar>
				<enableFlowNav>no</enableFlowNav>
				<enableChoiceNav>no</enableChoiceNav>
				<statusDisplay>separate</statusDisplay>
			</controls>
			<appearance>
				<courseStructureWidth>300</courseStructureWidth>
				<displayStage>
					<desired>
						<width>750</width>
						<height>550</height>
						<fullscreen>no</fullscreen>
					</desired>
					<required>
						<width>0</width>
						<height>0</height>
						<fullscreen>no</fullscreen>
					</required>
				</displayStage>
			</appearance>
			<behavior>
				<returnToLmsAction>suspend_all</returnToLmsAction>
				<disableRightClick>yes</disableRightClick>
				<preventWindowResize>no</preventWindowResize>
				<scoreRollupMode>average score of all units with scores</scoreRollupMode>
				<statusRollupMode>complete when all units satisfactorily complete</statusRollupMode>
				<firstScoIsPretest>no</firstScoIsPretest>
				<finishCausesImmediateCommit>no</finishCausesImmediateCommit>
				<invalidMenuItemAction>disable</invalidMenuItemAction>
				<alwaysFlowToFirstSco>yes</alwaysFlowToFirstSco>
				<logoutCausesPlayerExit>yes</logoutCausesPlayerExit>
				<resetRtTiming>never</resetRtTiming>
				<communications>
					<maxFailedSubmissions>2</maxFailedSubmissions>
					<commitFrequency>7894</commitFrequency>
				</communications>
				<debug>
					<controlAudit>false</controlAudit>
					<controlDetailed>false</controlDetailed>
					<runtimeAudit>true</runtimeAudit>
					<runtimeDetailed>true</runtimeDetailed>
					<sequencingAudit>false</sequencingAudit>
					<sequencingDetailed>false</sequencingDetailed>
					<lookaheadAudit>false</lookaheadAudit>
					<lookaheadDetailed>false</lookaheadDetailed>
					<includeTimestamps>true</includeTimestamps>
				</debug>
				<launch>
					<sco>frameset</sco>
					<player>frameset</player>
					<wrapScoWindowWithApi>no</wrapScoWindowWithApi>
				</launch>
				<exitActions>
					<intermediateSco>
						<satisfied>
							<normal>continue</normal>
							<suspend>message page</suspend>
							<timeout>message page</timeout>
							<logout>exit,confirmation</logout>
						</satisfied>
						<notSatisfied>
							<normal>message page</normal>
							<suspend>message page</suspend>
							<timeout>message page</timeout>
							<logout>exit,confirmation</logout>
						</notSatisfied>
					</intermediateSco>
					<finalSco>
						<satisfied>
							<normal>exit,no confirmation</normal>
							<suspend>exit,no confirmation</suspend>
							<timeout>exit,no confirmation</timeout>
							<logout>exit,no confirmation</logout>
						</satisfied>
						<notSatisfied>
							<normal>exit,no confirmation</normal>
							<suspend>exit,no confirmation</suspend>
							<timeout>exit,no confirmation</timeout>
							<logout>exit,no confirmation</logout>
						</notSatisfied>
					</finalSco>
				</exitActions>
			</behavior>
		</hstm:HSTMConfiguration>
	</technical>

